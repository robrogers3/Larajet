{
  "name": "robrogers/larajet",
  "type": "library",
  "description": "Laravel Mail Transport for Mailjet",
  "keywords": [
    "laravel",
    "mail",
    "mailjet",
    "mail driver"
  ],
  "homepage": "https://github.com/robrogers3/larajet",
  "license": "MIT",
  "authors": [
    {
      "name": "Rob Rogers",
      "email": "robrogers@me.com",
      "homepage": "https://github.com/robrogers3",
      "role": "Developer"
    }
  ],
    "repositories": [
    {
      "type": "path",
      "url": "/Users/rob.rogers/projects/laraldap-auth",
      "options": {
        "symlink": true
      }
    }
  ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": ">=5.1"
  },
  "require-dev": {
    "laravel/framework": ">=5.3",
    "phpunit/phpunit": "~4.0||~5.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "robrogers\\Larajet\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["tests/"]
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  }
}
